{
    "name": "puml2php/puml2php",
    "description": "Generate php code from plantuml files.",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "tasuku43",
            "email": "task.teckac@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tasuku43/puml-parser-php.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Puml2Php\\": "src/",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puml2Php\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "twig/twig": "^3.3",
        "puml2php/puml-parser": "^0.2.7"
    },
    "bin": [
        "puml2php"
    ]
}
